INCPATH  = -I/opt/sapdb/interfaces/odbc/incl -I/opt/sapdb/depend/incl -I$(QTDIR)/include -I$(QTDIR)/mkspecs/default
LINK     = g++
LFLAGS   = -shared -Wl,-soname,libqsqlsapdb.so.1
LIBS     = $(SUBLIBS) -L$(QTDIR)/lib -L/opt/sapdb/interfaces/odbc/lib -lsqlod -lqt
AR       = ar cqs
RANLIB   = 
