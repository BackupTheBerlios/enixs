####### Build rules

all: Makefile  $(TARGET)

$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC) $(SUBLIBS) 
	-rm -f $(TARGET)
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)



mocables: $(SRCMOC)

$(MOC): 
	( cd $(QTDIR)/src/moc ; $(MAKE) )

Makefile: qsapdb.pro  $(QTDIR)/mkspecs/default/qmake.conf 
	$(QMAKE) qsapdb.pro
qmake: 
	@$(QMAKE) qsapdb.pro

dist: 
	@mkdir -p obj/qsqlsapdb && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) obj/qsqlsapdb/ && ( cd `dirname obj/qsqlsapdb` && $(TAR) qsqlsapdb.tar qsqlsapdb && $(GZIP) qsqlsapdb.tar ) && mv `dirname obj/qsqlsapdb`/qsqlsapdb.tar.gz . && rm -rf obj/qsqlsapdb

mocclean:

uiclean:

clean:
	-rm -f $(OBJECTS) 
	-rm -f *~ core *.core


####### Sub-libraries

distclean: clean
	-rm -f $(TARGET) $(TARGET)
	

FORCE:

####### Compile

obj/plugin.o: plugin.cpp plugin.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o obj/plugin.o plugin.cpp

obj/driver.o: driver.cpp driver.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o obj/driver.o driver.cpp

obj/result.o: result.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o obj/result.o result.cpp

obj/util.o: util.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o obj/util.o util.cpp

####### Install

install_target: 
	@test -d $(INSTALL_ROOT)/usr/lib/qt3/plugins/sqldrivers/ || mkdir -p $(INSTALL_ROOT)/usr/lib/qt3/plugins/sqldrivers/
	-$(COPY) "$(TARGET)" "$(INSTALL_ROOT)/usr/lib/qt3/plugins/sqldrivers/$(TARGET)"
uninstall_target: 
	-$(DEL_FILE) "$(INSTALL_ROOT)/usr/lib/qt3/plugins/sqldrivers/$(TARGET)"
	-$(DEL_DIR) "$(INSTALL_ROOT)/usr/lib/qt3/plugins/sqldrivers/"

install: all install_target 

uninstall: uninstall_target 

