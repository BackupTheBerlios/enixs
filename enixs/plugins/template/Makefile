####### Build rules

all: Makefile  ../$(TARGET)

../$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC) $(SUBLIBS) 
	test -d ../ || mkdir -p ../
	-rm -f $(TARGET)
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)
	-mv $(TARGET) ../



mocables: $(SRCMOC)

$(MOC): 
	( cd $(QTDIR)/src/moc ; $(MAKE) )

Makefile: plugin.pro  $(QTDIR)/mkspecs/default/qmake.conf 
	$(QMAKE) plugin.pro
qmake: 
	@$(QMAKE) plugin.pro

dist: 
	@mkdir -p obj/template && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) obj/template/ && ( cd `dirname obj/template` && $(TAR) template.tar template && $(GZIP) template.tar ) && mv `dirname obj/template`/template.tar.gz . && rm -rf obj/template

mocclean:
	-rm -f $(OBJMOC)
	-rm -f $(SRCMOC)

uiclean:

clean: mocclean
	-rm -f $(OBJECTS) 
	-rm -f *~ core *.core


####### Sub-libraries

distclean: clean
	-rm -f ../$(TARGET) $(TARGET)
	

FORCE:

